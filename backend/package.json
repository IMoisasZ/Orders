{
  "name": "y",
  "version": "1.0.0",
  "description": "app to management orders of company injetaq",
  "license": "ISC",
  "author": "Moisas",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "start:dev": "cross-env NODE_ENV=development node --watch src/server.js",
    "start:prod": "cross-env NODE_ENV=production node --watch src/server.js",
    "test": "cross-env NODE_ENV=test jest",
    "start": "npm run start:dev"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "mysql2": "^3.14.2",
    "sequelize": "^6.37.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^30.0.5",
    "jsdoc": "^4.0.4",
    "supertest": "^7.1.4"
  },
  "jest": {
  "transform": {},
  "testMatch": [
    "<rootDir>/src/**/*.test.js",
    "<rootDir>/__tests__/**/*.js"
  ],
  "moduleNameMapper": {
    "^(\\.{1,2}/.*)\\.js$": "$1"
  }
}
}
